<%= render "components/autocomplete", {
  id: tag_field.id,
  name: "tags[#{tag_field.id}][]",
  label: {
    text: tag_field.label,
    bold: true
  },
  hint: tag_field.hint,
  options: LinkablesService.new(tag_field.document_type).select_options,
  selected_options: tags[tag_field.id],
  multiple: true,
  size: 10,
  data: {
    module: "autocomplete-multiselect",
    "contextual-guidance": contextual_guidance_id
  }
} %>
